.search-popup

	.container

		.search-popup__close((click)="close()")
			i.fas.fa-times

		.search-popup__search
			search-input([text]="text", [border]="true", [popup]="true", (onInput)="onInput($event)")

		.search-popup__results
			.search-popup__result(*ngFor="let breed of results")
				span {{ breed.name }}
